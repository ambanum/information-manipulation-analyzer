{
  "name": "information-manipulation-analyzer",
  "version": "0.0.0",
  "prettier": "prettier-config-ambanum",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "deploy:local:preproduction": "ansible-playbook ops/site.yml -i ops/inventories/dev.yml -e \"ENV=preproduction\"",
    "deploy:local:production": "ansible-playbook ops/site.yml -i ops/inventories/dev.yml -e \"ENV=production\"",
    "deploy:preproduction": "ansible-playbook ops/site.yml -i ops/inventories/production.yml -e \"ENV=preproduction\"",
    "deploy:production": "ansible-playbook ops/site.yml -i ops/inventories/production.yml -e \"ENV=production\"",
    "docker:build": "docker build -t $npm_package_name .",
    "docker:run": "(docker rm $npm_package_name || true) && docker run --rm -p 3000:3000 --name $npm_package_name -it $npm_package_name",
    "docker:ssh": "docker exec -it $npm_package_name /bin/bash"
  },
  "dependencies": {
    "@gouvfr/all": "^0.5.3",
    "next": "latest",
    "prettier-config-ambanum": "^1.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "typescript": "4.2.2"
  },
  "license": "MIT"
}
